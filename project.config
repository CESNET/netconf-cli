[project]
	description = A NETCONF-talking CLI
[access]
	inheritFrom = CzechLight/public-acl
[access "refs/*"]
	read = group CzechLight.netconf-cli.developers
[access "refs/for/*"]
	addPatchSet = group CzechLight.netconf-cli.developers
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group CzechLight.netconf-cli.developers
	label-Verified = -1..+1 group CESNET CI
	submit = group CzechLight.netconf-cli.developers
[access "refs/tags/*"]
	pushAnnotatedTag = group CzechLight.netconf-cli.developers
	createTag = group CzechLight.netconf-cli.developers
	createSignedTag = group CzechLight.netconf-cli.developers
[notify "developers"]
	email = group CzechLight.netconf-cli.developers
[Label "Verified"]
	function = MaxWithBlock
	value = -1 Fails
	value =  0 No score
	value = +1 Verified
	defaultValue = 0
[plugin "zuul-status"]
	url = https://gerrit.cesnet.cz/zuul/status/change/
